@model IEnumerable<reg_claim4.Models.logs>

@{
    ViewBag.Title = "ViewPage1";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>ViewPage1</h2>

<p>
    @Html.ActionLink("Создать Заявку", "addClaim")    
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.UserName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.events)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.dataTime)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.pc_ip)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.pc_name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Url)
        </th>
        <th></th>
    </tr>
    
@foreach (var item in ViewBag.Message) {

    <tr>
        
        <td>
            <p>@item.UserName</p>
        </td>
        <td>
            <p>@item.events</p>
        </td>
        <td>
            <p>@item.dataTime</p>
        </td>
        <td>
            <p>@item.pc_ip</p>
        </td>
        <td>
            <p>@item.pc_name</p>
        </td>
        <td>
            <p>@item.Url</p>
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>       
    </tr>
}
        
</table>
